    "def bt(L):\n",
    "    min_v = L[0]\n",
    "    max_p = 0\n",
    "    \n",
    "    for i in range(1,len(L)):\n",
    "        if L[i] < min_v:\n",
    "            min_v = L[i]\n",
    "        if L[i] > min_v:\n",
    "            max_p = max(max_p, L[i] - min_v)\n",
    "            \n",
    "    return max_p\n",
    "        \n",
    "bt([7,1,5,3,6,4]) #5\n",
    "bt([7,6,4,3,1]) #0"
   ]
